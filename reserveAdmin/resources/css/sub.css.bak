@charset "utf-8";


#Page {margin:0px auto; width:960px;}
	
	/*#AllMenu {background:#FFFFFF; width:958px; margin-top:35px; border:#e9e9e9 1px solid; border-top:0px; position:absolute; z-index:100;}*/
	#AllMenu {background:#FFFFFF; width:958px; margin-top:105px; border:#e9e9e9 1px solid; border-top:0px; position:absolute; z-index:100; clear:both;}
	#AllMenu ul {list-style:none;}
	#AllMenu ul li {display:inline; float:left; margin:15px 0px 0px 38px; padding-bottom:15px;}
	#AllMenu ul li dl {border-left:#d7d7d7 1px solid;}
	#AllMenu ul li dl dt {margin-bottom:15px;}
	#AllMenu ul li dl dd {margin-bottom:5px;}
	#AllMenu ul li dl dt a {border-left:#6eacdc 4px solid; padding-left:7px; color:#5a5a5a; font-weight:bold; margin-bottom:20px;}
	#AllMenu ul li dl dd a {padding-left:11px; color:#5a5a5a;}
	#AllMenu ul li dl dd a:hover {border-left:#6eacdc 4px solid; padding-left:7px; color:#5a5a5a;}

#PageTopLogo div {float: right; padding-top: 20px;}
#PageTopLogo div ul {list-style:none; text-align: right;}
#PageTopLogo div ul.oldVersion {margin-bottom: 10px; font-weight: bold;}
#PageTopLogo div ul li {display:inline;}


#PageTopMenu {background:url(../images/top_menu_bg.gif) no-repeat; clear:both; height:64px; float:left; /*width:960px;*/ width:1000px;}
	
	#PageTopMenu #MainMenu {list-style:none; height:35px;}
	#PageTopMenu #MainMenu li {display:inline; font-size:13px; font-weight:bold; line-height:35px; margin-left:10px; float:left; background:url(../images/bar.gif) no-repeat 0% 50%; padding-left:11px;}
	/*#PageTopMenu #MainMenu li {display:inline; font-size:14px; font-weight:bold; line-height:35px; margin-left:30px; float:left;}*/
	#PageTopMenu #MainMenu li a {color:#f2f2f2; padding:5px 15px 15px 15px;}
	#PageTopMenu #MainMenu li a:hover {color:#333333; background:#f4f4f4; padding:5px 15px 15px 15px;}
	
	#PageTopMenu ul {list-style:none; height:29px;}
	#PageTopMenu ul li {display:inline; font-size:12px; font-weight:bold; line-height:29px; margin-left:20px; font-weight:normal;}
	#PageTopMenu ul li a {color:#333333;}
	#PageTopMenu ul li a:hover {color:#2d85c3;}

#PageLeft {width:200px;min-height:550px; _height:550px; float:left;}

	#PageLeftTop {border:#d7d7d7 1px solid; clear:both; padding:10px;}
		#PageLeftTop ul {list-style:none; height:80px;}
		#PageLeftTop ul li {float:left;}
		#PageLeftTop ul li dl {float:left; margin-left:10px;}
		#PageLeftTop ul li dl dt {font-size:12px; color:#76a6d6; font-weight:bold;}
		
		#PageLeftMemo {background:url(../images/memo_bg.gif) no-repeat; width:196px; height:193px; padding:2px; margin-top:10px;}
		#PageLeftMemo dl dd {background:url(../images/list_bl.gif) no-repeat 0% 50%; padding-left:10px; margin-left:17px; margin-bottom:5px;}
		
		#PageLeftMenu ul {list-style:none;}
            #titleBlock {float: left; width: 200px; height: 64px; background: url(../images/left_menu/left_title.gif); margin-bottom: 15px; font-size: 22px; box-sizing: border-box; padding-left: 5px; color: #6C5841; line-height: 80px; font-weight: bold;}

            #leftMenuCategory a {float: left; width: 200px; height: 37px; padding-left: 5px; box-sizing: border-box; font-size: 16px; line-height: 37px; color: #888; background: url(../images/left_menu/left_menu_back.gif);}
            #leftMenuCategory a:hover {background: url(../images/left_menu/left_menu_back_over.gif); color: #FFF;}
            #leftMenuCategory a.thisMenu {float: left; width: 200px; height: 37px; padding-left: 5px; box-sizing: border-box; font-size: 15px; line-height: 37px; color: #FFF; background: url(../images/left_menu/left_menu_back_over.gif);}
		
	#PageRight {width:737px; float:left; padding-left:23px;}
	#PageRightTop {height: 25px; margin-bottom:12px; font-size: 18px; font-weight: bold; color: #6C5841; box-sizing: border-box; background: url("../images/left_menu/cont_title.gif") no-repeat; background-size: auto; padding-left: 17px; line-height: 25px;}
	#PageRightSearch {border:#d7d7d7 1px solid; padding:10px 20px; margin-bottom:25px; text-align:left; float:left; width:695px;}
	#PageRightSearch .icon_01 img {margin-left:5px;}
	
	
	#PageRightContent {clear:both;}
	
	#PageRight_1 dl {float:left; padding:0px 20px; margin-bottom:20px}
	#PageRight_1 dt {margin-bottom:5px;}
	#PageRight_1 dd {background:url(../images/list_bl.gif) no-repeat 0% 50%; padding-left:10px; margin-bottom:5px; float:left;}
	#PageRight_1 dd span {float:right;}
	
	#PageRight_2 {clear:both;}
	#PageRight_2 dl {padding:0px 20px;}
	#PageRight_2 dd {background:url(../images/mail_send_bg.gif); padding:12px; margin-bottom:5px;}
	#PageRight_2 dd table {width:100%;}
	#PageRight_2 dd table th {border:0px; background:#FFFFFF; padding:5px; text-align:center;}
	#PageRight_2 dd table td {border:0px; border-top:#ededed 1px solid; background:#FFFFFF; padding:5px 15px; text-align:center;}
	
	#PageRight_3 dl {float:left; padding:0px 20px; margin:20px 0px;}
	#PageRight_3 dt {margin-bottom:5px;}
	#PageRight_3 dd {background:url(../images/list_bl.gif) no-repeat 0% 50%; padding-left:10px; margin-bottom:5px; float:left;}
	#PageRight_3 dd span {float:right;}

    .mainBoardTitle dl dt {background: url(/images/left_menu/cont_title.gif) no-repeat; width:100%; box-sizing: border-box; height: 35px; padding-left: 15px; font-size: 15px; line-height: 35px; font-weight: bold; background-position: 0 45%; vertical-align: middle; border-bottom: 1px solid #D7D7D7;}
    .mainBoardTitle dl dt a {float: right;}

#PageCopyright {clear:both; border-top:#eaeae8 1px solid; background:url(../images/copy_logo.gif) no-repeat; font-size:11px; color:#a8a8a8; height:100px; padding:25px 0px 0px 170px; position: relative;}



.111classTableBase tr td input.bill_list_in_button {width: 59px; height: 19px; text-align: center; font-size: 12px; color: #A9A9A9; background: #FFF;}

